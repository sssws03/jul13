<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board">
	<select id="boardList" resultType="com.sssws03.web.BoardDTO">
		SELECT * 
		FROM board
		ORDER BY bno DESC 
		LIMIT 10
	</select>


</mapper>